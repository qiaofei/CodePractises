<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="diary" extends="json-default" namespace="/diary">
		<action name="getDiaryInfoById" class="diaryInfoAction" method="getDiaryInfoById">
			<result type="json">
				<param name="root">transMsg</param>
			</result>
		</action>
		<action name="insertNewDiary" class="diaryInfoAction" method="insertNewDiary">
			<result type="json">
				<param name="root">transMsg</param>
			</result>
		</action>
		<action name="updateDiary" class="diaryInfoAction" method="updateDiary">
			<result type="json">
				<param name="root">transMsg</param>
			</result>
		</action>
		<action name="deleteDiary" class="diaryInfoAction" method="deleteDiary">
			<result type="json">
				<param name="root">transMsg</param>
			</result>
		</action>
	</package>
</struts>